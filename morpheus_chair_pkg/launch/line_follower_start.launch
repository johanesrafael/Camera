<?xml version="1.0" encoding="UTF-8"?>
<launch>

        <arg name="R" default="255" />
        <arg name="G" default="255" />
        <arg name="B" default="255" />
        <arg name="mode" default="colour_cal" />

        <node name="line_follower_start"
            pkg="morpheus_chair_pkg"
            type="line_follower.py"
            respawn="false" output="screen" args="$(arg R) $(arg G) $(arg B) $(arg mode)">
        </node>
</launch>